{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "eofline": true,
    "max-line-length": [
      true,
      120
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-unused-variable": true,
    "member-ordering": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "trailing-comma": false,
    "no-empty-interface": false,
    "quotemark": {
      "severity": "error",
      "options": [
        "single",
        "jsx-double"
      ]
    },
    "linebreak-style": [true, "LF"]
  },
  "rulesDirectory": []
}